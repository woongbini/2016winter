import javax.swing.JFrame;
import javax.swing.JPanel;

public class MyFrame extends JFrame {
	
	public MyFrame() {
		setSize(300, 300);
		setTitle("swing frame");
		this.setContentPane(new MyPanel());
		setVisible(true);
	}
	
	class MyPanel extends JPanel {
		public MyPanel() {
			
		}
	}
	
	public static void main(String[] args) {
		new MyFrame();
	}
}
